{
  "name": "@repo/api",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "tsc -b -v",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "aggregations": [
        "src/aggregations/index"
      ],
      "transactions": [
        "src/transactions/index"
      ],
      "pagination": [
        "src/pagination/index"
      ],
      "*": [
        "src/*"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./transactions": {
      "import": "./dist/transactions/index.js",
      "require": "./dist/transactions/index.js"
    },
    "./aggregations": {
      "import": "./dist/aggregations/index.js",
      "require": "./dist/aggregations/index.js"
    },
    "./pagination": {
      "import": "./dist/pagination/index.js",
      "require": "./dist/pagination/index.js"
    },
    "./*": {
      "import": "./dist/*/index.js",
      "require": "./dist/*/index.js"
    }
  },
  "dependencies": {
    "@nestjs/mapped-types": "*",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^20.3.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4"
  }
}
